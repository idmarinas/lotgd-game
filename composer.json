{
    "name": "idmarinas/lotgd",
    "description": "A new version of The Legend of the Green Dragon, upgrade scripts and optimization, LoTGD Core is a Symfony Application",
    "license": "CC-BY-NC-SA-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-session": "*",
        "beberlei/doctrineextensions": "^1.2",
        "bukashk0zzz/filter-bundle": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.1",
        "cron/cron-bundle": "^2.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "ezyang/htmlpurifier": "^4.13",
        "gedmo/doctrine-extensions": "^3.0",
        "jaxon-php/jaxon-core": "^3.2",
        "jaxon-php/jaxon-dialogs": "^3.1",
        "jaxon-php/jaxon-symfony": "^3.2",
        "knplabs/knp-menu-bundle": "^3.1",
        "laminas/laminas-escaper": "^2.7",
        "laminas/laminas-filter": "^2.10",
        "laminas/laminas-hydrator": "^4.1",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-math": "^3.3",
        "laminas/laminas-paginator": "^2.9",
        "laminas/laminas-servicemanager": "^3.6",
        "laminas/laminas-view": "^2.12",
        "marioblazek/twig-byte-units-extension": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.1",
        "snipe/banbuilder": "^2.3",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/block-bundle": "^4.5",
        "stof/doctrine-extensions-bundle": "^1.5",
        "sylius/theme-bundle": "^2.1",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/lock": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "5.2.*",
        "symfonycasts/reset-password-bundle": "^1.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/web-profiler-bundle": "^5.2"
    },
    "suggest": {
        "idmarinas/advertising-bundle": "AdvertisingBundle if you want add Adsense Network to your server. Can create custom networks."
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 5000,
        "sort-packages": true,
        "platform": {
            "php": "7.3.0"
        }
    },
    "autoload": {
        "psr-4": {
            "Lotgd\\Core\\": "src/"
        },
        "exclude-from-classmap": [
            "src/ajax/",
            "src/core/",
            "src/local/"
        ],
		"files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Lotgd\\Core\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lotgd:prod:install": [
            "composer install --no-dev --no-interaction --no-plugins --no-scripts",
            "composer dump-env prod",
            "php bin/console cache:clear"
        ],
        "test": "php bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.me/idmarinas"
        }
    ]
}
