{
    "name": "idmarinas/lotgd",
    "description": "A new version of The Legend of the Green Dragon, upgrade scripts and optimization for PHP >= 7.0",
    "config": {
        "process-timeout": 5000,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "CC-BY-NC-SA-2.0",
    "require": {
        "php": "^7.1.*",
        "doctrine/orm": "^2.5",
        "ezyang/htmlpurifier": "^4.9",
        "hellogerard/jobby": "^3.4",
        "jaxon-php/jaxon-core": "^2.2",
        "jaxon-php/jaxon-dialogs": "^1.2",
        "symfony/filesystem": "^3.4",
        "twig/twig": "^2.5",
        "zendframework/zend-cache": "^2.8",
        "zendframework/zend-code": "^3.1",
        "zendframework/zend-config": "^3.2",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-debug": "^2.6",
        "zendframework/zend-escaper": "^2.6",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-http": "^2.8",
        "zendframework/zend-hydrator": "^2.4",
        "zendframework/zend-i18n": "^2.9",
        "zendframework/zend-log": "^2.10",
        "zendframework/zend-mail": "^2.10",
        "zendframework/zend-math": "^3.1",
        "zendframework/zend-paginator": "^2.8",
        "zendframework/zend-serializer": "^2.9",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.2"
    },
    "require-dev": {
        "phan/phan": "^1.1",
        "psecio/iniscan": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Lotgd\\Core\\": "src/core",
            "Lotgd\\Local\\": "src/local",
            "Lotgd\\Ajax\\Core\\": "scr/ajax/core",
            "Lotgd\\Ajax\\Local\\": "scr/ajax/local"
        }
    }
}
