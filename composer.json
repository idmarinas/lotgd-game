{
    "name": "idmarinas/lotgd",
    "description": "A new version of The Legend of the Green Dragon, upgrade scripts and optimization for PHP >= 7.2",
    "config": {
        "process-timeout": 5000,
        "sort-packages": true,
        "platform": {
            "php": "7.2.25"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "CC-BY-NC-SA-2.0",
    "require": {
        "php": ">=7.2",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-session": "*",
        "beberlei/doctrineextensions": "^1.2",
        "bukashk0zzz/filter-bundle": "^4.0",
        "cocur/slugify": "^3.2",
        "composer/semver": "^1.5",
        "doctrine/doctrine-orm-module": "^2.1",
        "doctrine/orm": "^2.6",
        "ezyang/htmlpurifier": "^4.13",
        "gedmo/doctrine-extensions": "^2.4",
        "hellogerard/jobby": "^3.4",
        "jaxon-php/jaxon-core": "^2.2",
        "jaxon-php/jaxon-dialogs": "^1.2",
        "laminas/laminas-cache": "^2.8",
        "laminas/laminas-code": "^3.3",
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-db": "^2.10",
        "laminas/laminas-dependency-plugin": "^1.0",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-http": "^2.10",
        "laminas/laminas-hydrator": "^2.4",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-log": "^2.12",
        "laminas/laminas-mail": "^2.10",
        "laminas/laminas-math": "^3.2",
        "laminas/laminas-paginator": "^2.8",
        "laminas/laminas-serializer": "^2.9",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-session": "^2.9",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-view": "^2.11",
        "macfja/tracy-doctrine-sql": "dev-master",
        "marioblazek/twig-byte-units-extension": "^1.0",
        "snipe/banbuilder": "^2.3",
        "symfony/console": "^4.4",
        "symfony/doctrine-bridge": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/form": "^4.4",
        "symfony/options-resolver": "^4.4",
        "symfony/security-csrf": "^4.4",
        "symfony/twig-bridge": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4",
        "tracy/tracy": "^2.6",
        "twig/twig": "^2.11"
    },
    "require-dev": {
        "milo/vendor-versions": "^1.1",
        "phan/phan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Lotgd\\": "src/",
            "Lotgd\\Core\\": "src/core/",
            "Lotgd\\Local\\": "src/local/",
            "Lotgd\\Ajax\\Core\\": "src/ajax/core/",
            "Lotgd\\Ajax\\Local\\": "src/ajax/local/"
        }
    }
}
