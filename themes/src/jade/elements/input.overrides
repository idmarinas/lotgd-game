/*******************************
         Theme Overrides
*******************************/

.ui.action.input:not([class*="left action"]) > .dropdown:first-child:not(:last-child),
.ui.action.input:not([class*="left action"]) > .button:first-child:not(:last-child),
.ui.action.input:not([class*="left action"]) > .buttons:first-child:not(:last-child) > .button
{
    border-radius: @borderRadius 0 0 @borderRadius;
}
